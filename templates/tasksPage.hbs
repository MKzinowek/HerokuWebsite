  <script defer src="js/main.js"></script>

  {{>head}}

</head>

<body>
  <div class="card" id="taskArea" style="width: 18rem;">
    <div class="card-body">
      <h5 class="card-title">Tasks</h5>
    </div>
    <ul class="list-group list-group-flush" id="taskArea">
      <p class="list-group-item" id="title"></p>
      <p class="list-group-item" id="completed"></p>
      <p class="list-group-item" id="description"></p>
    </ul>
    <div class="card-body">
      <a class="get-task" id="viewTasks">Get Task</a>
      <a class="card-link">Next</a>
    </div>
</body>

</html>